{
  "manifest_version": 2,

  "name": "Plock",
  "description": "Ad Blocker",
  "version": "0.1",

  "browser_action": {
    "default_icon": "plock.png",
    "default_title": "Plock"
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "background",
    "tabs"
  ],
  "background": {
    "scripts": ["plock.js"]
  }
}